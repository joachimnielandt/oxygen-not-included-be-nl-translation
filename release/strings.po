msgstr "<link=\"CEILINGLIGHT\">Plafondlamp</link>"

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.FAUXPIPE.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.FAUXPIPE.DESC"
msgid "The height of plumbing-inspired interior design."
msgstr "Het toppunt van op sanitair geïnspireerd interieurontwerp."

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.FAUXPIPE.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.FAUXPIPE.NAME"
msgid "<link=\"CEILINGLIGHT\">Faux Pipe Ceiling Light</link>"
msgstr "<link=\"CEILINGLIGHT\">Faux Leiding Plafondlamp</link>"

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.LABFLASK.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.LABFLASK.DESC"
msgid "For best results, do not fill with liquids."
msgstr "Voor het beste resultaat niet vullen met vloeistoffen."

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.FACADES.LABFLASK.NAME
