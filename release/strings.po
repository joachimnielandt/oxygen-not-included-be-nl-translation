msgid "<link=\"COLDBREATHERCLUSTER\">Wheezewort</link>"
msgstr "<link=\"COLDBREATHERCLUSTER\">Piepkruid</link>"

#. STRINGS.CREATURES.SPECIES.COLDWHEAT.DESC
msgctxt "STRINGS.CREATURES.SPECIES.COLDWHEAT.DESC"
msgid "Sleet Wheat produces <link=\"COLDWHEATSEED\">Sleet Wheat Grain</link>, a chilly grain that can be processed into <link=\"FOOD\">Food</link>."
msgstr "Sneeuwtarwe produceert <link=\"COLDWHEATSEED\">Sneeuwtarwe Graan</link>, een kille graansoort die kan worden verwerkt tot <link=\"FOOD\">Voedsel</link>."

